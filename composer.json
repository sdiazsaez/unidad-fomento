{
    "name": "larangular/unidad-fomento",
    "description": "Laravel scrapper and backup for local CLF to CLP currency",
    "license": "MIT",
    "authors": [
        {
            "name": "Simon Diaz",
            "email": "sdiaz.sz@gmail.com"
        }
    ],
    "require": {
        "php": "^7.1.3",
        "illuminate/support": "5.*",
        "larangular/support": "^0.1.3",
        "larangular/routing-controller": "^0.1.2",
        "larangular/uf-scraper": "^0.1.1",
        "larangular/migration-package": "^0.1.4"
    },
    "autoload": {
        "psr-4": {
            "Larangular\\UnidadFomento\\": "./src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Larangular\\UnidadFomento\\Tests\\": "./tests"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Larangular\\UnidadFomento\\UnidadFomentoServiceProvider"
            ],
            "aliases": {
                "UFController": "Larangular\\UnidadFomento\\Facades\\UnidadFomentoController"
            }
        }
    },
    "require-dev": {
        "orchestra/testbench": "^3.7"
    }
}
